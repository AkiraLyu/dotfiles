# Maintainer: Akira <akira@example.com>
pkgname=wine-meta
pkgver=11.0
pkgrel=1
pkgdesc="Meta package for wine and related addons"
arch=('any')
license=('custom:none')

depends=(
    wine
    wine-gecko
    wine-mono
    winetricks
)

package() {
    mkdir -p "$pkgdir/usr/share/doc/$pkgname"
    echo "Meta package for fcitx5 and related addons" > "$pkgdir/usr/share/doc/$pkgname/README"
}

